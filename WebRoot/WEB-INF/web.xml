<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Daxigua</display-name>
  <welcome-file-list>
    <welcome-file>user_index.html</welcome-file>
    <welcome-file>Sys_index.htm</welcome-file>
  </welcome-file-list>
  <!-- 
  <taglib>
  	<taglib-uri>/tags/Mypager.tld</taglib-uri>
  	<taglib-location>/WEB-INF/Mypager.tld</taglib-location>
  </taglib>
   -->
	<servlet>
		<servlet-name>code</servlet-name>
		<servlet-class>daxigua.servlet.CodeServlet</servlet-class>
	</servlet>
    <servlet-mapping>
    	<servlet-name>code</servlet-name>
    	<url-pattern>/create.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
		<servlet-name>del</servlet-name>
		<servlet-class>daxigua.servlet.delServlet</servlet-class>
	</servlet>
    <servlet-mapping>
    	<servlet-name>del</servlet-name>
    	<url-pattern>/del.act</url-pattern>
    </servlet-mapping>
    
    <servlet>
		<servlet-name>add</servlet-name>
		<servlet-class>daxigua.servlet.addServlet</servlet-class>
	</servlet>
    <servlet-mapping>
    	<servlet-name>add</servlet-name>
    	<url-pattern>/newuser.act</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
		<servlet-name>UrltoPicServlet</servlet-name>
		<servlet-class>daxigua.servlet.UrltoPicServlet</servlet-class>
	</servlet>
    <servlet-mapping>
    	<servlet-name>UrltoPicServlet</servlet-name>
    	<url-pattern>/pic_deal.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
		<servlet-name>doPage</servlet-name>
		<servlet-class>daxigua.servlet.doPageServlet</servlet-class>
	</servlet>
    <servlet-mapping>
    	<servlet-name>doPage</servlet-name>
    	<url-pattern>/page.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
		<servlet-name>logServlet</servlet-name>
		<servlet-class>daxigua.servlet.LogServlet</servlet-class>
	</servlet>
    <servlet-mapping>
    	<servlet-name>logServlet</servlet-name>
    	<url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
		<servlet-name>SyslogServlet</servlet-name>
		<servlet-class>daxigua.servlet.SyslogServlet</servlet-class>
	</servlet>
    <servlet-mapping>
    	<servlet-name>SyslogServlet</servlet-name>
    	<url-pattern>/Syslogin.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
    <servlet-name>uploadServlet</servlet-name>
    <servlet-class>daxigua.servlet.uploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>uploadServlet</servlet-name>
    <url-pattern>/upload.do</url-pattern>
    </servlet-mapping>
    
    
    

</web-app>